@using HomeERP.Models.EAV.Domain
@using Object = HomeERP.Models.EAV.Domain.Object
@model Entity

<div class="accordion-item">
    <h2 class="accordion-header">
        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapse@(TempData["Counter"])" aria-controls="collapse@(TempData["Counter"])">
            @Model.Name
        </button>
    </h2>
    <div id="collapse@(TempData["Counter"])" class="accordion-collapse collapse" data-bs-parent="#accordionExample">
        <div class="accordion-body">
            <div class="list-group">
                @foreach (Object Object in Model.Objects)
                {
                    <button onclick="GetObjectFiles('@Object.Id')" class="list-group-item list-group-item-action">@Object.Name</button>
                }
            </div>
        </div>
    </div>
</div>
